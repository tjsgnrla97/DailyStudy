
public class forTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 0;
		for(int i=0; i<100; i++) {
			if((i+1)%4==0)
				number += (i+1);
		}
		System.out.println(number);
		
		int total = 0;
		for (int i=4; i<=100; i+=4) {
			total += i;
		}
		System.out.println(total);
	}

}
