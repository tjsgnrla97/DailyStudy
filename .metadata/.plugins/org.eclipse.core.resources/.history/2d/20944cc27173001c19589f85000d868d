
public class arrayTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//배열 선언 및 생성
		int[] arr = new int[4];
		for(int i=0; i<= arr.length; i++) {
			System.out.println(arr[i]);
		}
		//new int[]{1,2,3,4,5} new int[] 생략됨
		int[] su = {1,2,3,4,5};
		for(int s:su) {
			System.out.println(s);
		}
	}
	//int[] arr; arr=null;
}
